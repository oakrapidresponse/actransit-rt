{
    "python.defaultInterpreterPath": ".venv/bin/python3",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "files.watcherExclude": {
        "**/.mypy_cache/**": true,
        "**/.tox/**": true,
        "**/.venv/**": true,
        "**/out/**": true
    }
}
